{
  "name": "coding-challenge",
  "version": "1.0.0",
  "description": "An App that displays selection for a country and a state base on selected country.",
  "main": "index.js",
  "scripts": {
    "start": "npm run prettier && npm run compile && npm run serve",
    "test": "jest",
    "serve": "concurrently \"http-server ./src -p 8080 -o\"",
    "compile": "npx sass src/styles/base.scss src/styles/base.css && npx tsc",
    "prettier": "npx prettier . --check && npx prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vashfox/coding-challenge.git"
  },
  "keywords": [
    "countries",
    "states"
  ],
  "author": "Francis A.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vashfox/coding-challenge/issues"
  },
  "homepage": "https://github.com/vashfox/coding-challenge#readme",
  "dependencies": {
    "jest-environment-jsdom": "^29.7.0",
    "scss": "^0.2.4",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2"
  }
}
